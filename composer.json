{
  "name": "tentwofour/twig",
  "type": "library",
  "description": "Twig extensions",
  "keywords": [
    "ten24",
    "tentwofour",
    "twig"
  ],
  "homepage": "https://github.com/tentwofour/twig",
  "license": "MIT",
  "authors": [
    {
      "name": "Blair D. Patterson",
      "homepage": "http://tentwentyfour.ca",
      "email": "blair@tentwentyfour.ca"
    }
  ],
  "require": {
    "php": "^7.0",
    "twig/twig": "^1.41|^2.10",
    "tentwofour/formatter": "^1.1",
    "tentwofour/inflector": "^1.1",
    "twig/extensions": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Ten24\\Twig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ten24\\Tests\\Twig\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
